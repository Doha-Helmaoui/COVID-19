"use strict";function openLinkWithTranslation(culture){var url=$('[data-custom-language-selector="'+culture+'"]').val();null==url&&(url="/"+culture),window.location=url}!function($){$(function(){$("input[data-custom-language-selector]").each(function(){var culture=$(this).data("custom-language-selector");$("a[data-available-culture]").each(function(index,element){$(this).data("available-culture")==culture&&$(element).css("display","inline-block")})}),$("a[data-available-culture][class=' header']").each(function(index,element){"none"==element.style.display&&$(element).addClass("disabled")})})}(jQuery);